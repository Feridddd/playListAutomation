{
  "name": "playlisttestsautomation",
  "version": "1.0.0",
  "description": "Automated UI tests for Playlist App using Playwright",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:report": "playwright test --reporter=list",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "playwright",
    "ui-testing",
    "playlist-app",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.1"
  }
}
